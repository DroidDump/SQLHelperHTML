me_in VARCHAR(50) )\r\nRETURNS INT READS SQL DATA\r\n\r\nBEGIN\r\n\r\n   DECLARE done INT DEFAULT FALSE;\r\n   DECLARE TotalSize INT DEFAULT 0;\r\n\r\n   DECLARE c1 CURSOR FOR\r\n     SELECT SUM(file_size)\r\n     FROM pages\r\n      WHERE site_name = name_in;\r\n\r\n   DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE;\r\n\r\n   OPEN c1;\r\n   FETCH c1 INTO TotalSize;\r\n\r\n   CLOSE c1;\r\n\r\n   RETURN TotalSize;\r\n\r\nEND; \/\/\r\n\r\nDELIMITER ;<\/pre>\n<p>  \u0417\u0430\u0442\u0435\u043c \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0432\u044b\u0437\u0432\u0430\u0442\u044c \u0432\u0430\u0448\u0443 \u043d\u043e\u0432\u0443\u044e \u0444\u0443\u043d\u043a\u0446\u0438\u044e (\u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442 \u043a\u0443\u0440\u0441\u043e\u0440) \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c:<\/p>\n<pre class=\"lang:pgsql decode:true \" >SELECT FindSize ('Google.com');<\/pre>\n","protected":false},"excerpt":{"rendered":"<p>\u0412 \u044d\u0442\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u043e\u043c \u043f\u043e\u0441\u043e\u0431\u0438\u0438 \u0432\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435, \u043a\u0430\u043a \u043e\u0431\u044a\u044f\u0432\u0438\u0442\u044c \u043a\u0443\u0440\u0441\u043e\u0440 \u0432 MariaDB \u0441 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441\u043e\u043c \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438.<\/p>\n","protected":false},"author":1,"featured_media":0,"comment_status":"closed","ping_status":"closed","sticky":false,"template":"","format":"standard","meta":[],"categories":[282],"tags":[],"_links":{"self":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts\/6642"}],"collection":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2