\u043d\u043a\u0446\u0438\u044e TIME_FORMAT \u0432 MySQL.<br \/>\n\u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440:<\/p>\n<pre class=\"lang:mysql decode:true \" >\r\nmysql> SELECT TIME_FORMAT('', '%H %i %s');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  18 10 33                            \r\n\r\nmysql> SELECT TIME_FORMAT('', '%h:%i:%s %p');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  06:10:33 PM                            \r\n\r\nmysql> SELECT TIME_FORMAT('', '%h:%i%p');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  06:10PM                            \r\n\r\nmysql> SELECT TIME_FORMAT('15:46:03.000001', '%r');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  03:46:03 PM                          \r\n\r\nmysql> SELECT TIME_FORMAT('15:46:03.000001', '%T');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  15:46:03                             \r\n\r\nmysql> SELECT TIME_FORMAT('05:46:03.000001', '%f');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  000001\r\n<\/pre>\n","protected":false},"excerpt":{"rendered":"<p>\u0412 \u044d\u0442\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u043e\u043c \u043f\u043e\u0441\u043e\u0431\u0438\u0438 \u0432\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435, \u043a\u0430\u043a \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c MySQL \u0444\u0443\u043d\u043a\u0446\u0438\u044e TIME_FORMAT \u0441 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441\u043e\u043c \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438.<\/p>\n","protected":false},"author":1,"featured_media":0,"comment_status":"closed","ping_status":"closed","sticky":false,"template":"","format":"standard","meta":[],"categories":[27],"tags":[],"_links":{"self":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts\/2956"}],"collection":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/oracleplsql.ru\/wp-jso