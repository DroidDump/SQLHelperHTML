 \u0442\u043e <span class=\"arg\">highest_number<\/span> \u0438 <span class=\"arg\">lowest_number<\/span> \u0431\u0443\u0434\u0443\u0442 \u0438\u043c\u0435\u0442\u044c \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0441\u0442\u0430\u0440\u0442\u043e\u0432\u043e\u0435 \u0438 \u043a\u043e\u043d\u0435\u0447\u043d\u043e\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435 \u0434\u043b\u044f <span class=\"arg\">loop_counter<\/span>.<\/li>\n<li>\u0421\u043c. \u0442\u0430\u043a\u0436\u0435 <a href=\"https:\/\/oracleplsql.ru\/while-loop.html\">WHILE LOOP<\/a>, <a href=\"https:\/\/oracleplsql.ru\/exit.html\">EXIT WHEN<\/a>.<\/li>\n<p>.<\/ul>\n<h2>\u041f\u0440\u0438\u043c\u0435\u0440<\/h2>\n<pre class=\"lang:plsql decode:true \" >\r\nFOR Lcntr IN 1..20\r\nLOOP\r\n   LCalc := Lcntr * 31;\r\nEND LOOP;\r\n<\/pre>\n<p>\u042d\u0442\u043e\u0442 \u043f\u0440\u0438\u043c\u0435\u0440 \u0446\u0438\u043a\u043b\u0430 \u0432\u044b\u043f\u043e\u043b\u043d\u0438\u0442\u0441\u044f 20 \u0440\u0430\u0437. \u0421\u0447\u0435\u0442\u0447\u0438\u043a \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f LCntr  \u043d\u0430\u0447\u043d\u0435\u0442\u0441\u044f \u0441 1 \u0438 \u0437\u0430\u043a\u043e\u043d\u0447\u0438\u0442\u0441\u044f 20.<\/p>\n<pre class=\"lang:plsql decode:true \" >\r\ndeclare\r\nLCalc number;\r\nbegin\r\nFOR Lcntr IN 1..20\r\nLOOP\r\n   LCalc := Lcntr * 31;\r\n   dbms_output.put_line(LCalc);   \r\nEND LOOP;\r\nend;\r\n\r\n--\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\r\n\r\n31\r\n62\r\n93\r\n124\r\n155\r\n186\r\n217\r\n248\r\n279\r\n310\r\n341\r\n372\r\n403\r\n434\r\n465\r\n496\r\n527\r\n558\r\n589\r\n620\r\n<\/pre>\n<p>\u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043c\u043e\u0434\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u0440 <span class=\"arg\">REVERSE<\/span> \u0434\u043b\u044f \u0437\u0430\u043f\u0443\u0441\u043a\u0430 \u0446\u0438\u04