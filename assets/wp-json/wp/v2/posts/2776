                 \r\n\r\nmysql> SELECT TRIM(TRAILING ' ' FROM '  Adam and Eva  ');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  Adam and Eva                             \r\n\r\nmysql> SELECT TRIM(BOTH ' ' FROM '  Adam and Eva  ');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  Adam and Eva                           \r\n\r\nmysql> SELECT TRIM(' ' FROM '  Adam and Eva  ');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  Adam and Eva                      \r\n\r\n\r\nmysql> SELECT TRIM('   Adam and Eva   ');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  Adam and Eva               \r\n\r\nmysql> SELECT TRIM(LEADING '2' FROM '22er4');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  er4                            \r\n\r\nmysql> SELECT TRIM(TRAILING '9' FROM 'Disco999');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:  Disco                              \r\n\r\nmysql> SELECT TRIM(BOTH '123' FROM '123Adam123');\r\n\r\n#\u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442:   Adam                               \r\n<\/pre>\n","protected":false},"excerpt":{"rendered":"<p>\u0412 \u044d\u0442\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u043e\u043c \u043f\u043e\u0441\u043e\u0431\u0438\u0438 \u0432\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435, \u043a\u0430\u043a \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c MySQL \u0444\u0443\u043d\u043a\u0446\u0438\u044e TRIM \u0441 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441\u043e\u043c \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438.<\/p>\n","protected":false},"author":1,"featured_media":0,"comment_status":"closed","ping_status":"closed","sticky":false,"template":"","format":"standard","meta":[],"categories":[24],"tags":[],"_links":{"self":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts\/2776"}],"collection":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts"}],"abou