u0435\u0432\u0434\u043e\u043d\u0438\u043c\u044b \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442\u0441\u044f \u0434\u043b\u044f \u043e\u0431\u043b\u0435\u0433\u0447\u0435\u043d\u0438\u044f \u0447\u0442\u0435\u043d\u0438\u044f \u0437\u0430\u0433\u043e\u043b\u043e\u0432\u043a\u043e\u0432 \u0441\u0442\u043e\u043b\u0431\u0446\u043e\u0432 \u0432 \u043d\u0430\u0431\u043e\u0440\u0435 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u0432 \u0432 MariaDB. \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043f\u0440\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0438 \u0444\u0443\u043d\u043a\u0446\u0438\u0438 COUNT \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0430 \u0444\u0443\u043d\u043a\u0446\u0438\u0438 COUNT.<br \/>\n  \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440:<\/p>\n<pre class=\"lang:pgsql decode:true \" >SELECT site_name, COUNT(site_id) AS Total\r\nFROM sites\r\nGROUP BY site_name;<\/pre>\n<p>  \u0412 \u044d\u0442\u043e\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u0435 \u043c\u044b \u043d\u0430\u0437\u0432\u0430\u043b\u0438 \u043f\u043e\u043b\u0435 COUNT (site_id) \u043a\u0430\u043a \"Total\". \u0412 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0435 \"Total\" \u0431\u0443\u0434\u0435\u0442 \u043e\u0442\u043e\u0431\u0440\u0430\u0436\u0430\u0442\u044c\u0441\u044f \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0437\u0430\u0433\u043e\u043b\u043e\u0432\u043a\u0430 \u0434\u043b\u044f \u0432\u0442\u043e\u0440\u043e\u0433\u043e \u0441\u0442\u043e\u043b\u0431\u0446\u0430, \u043a\u043e\u0433\u0434\u0430 \u0431\u0443\u0434\u0435\u0442 \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0435\u043d \u043d\u0430\u0431\u043e\u0440 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u0432. \u041f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u0432 \u043d\u0430\u0448\u0435\u043c <span class=\"arg\">alias_name<\/span> \u043d\u0435 \u0431\u044b\u043b\u043e \u043f\u0440\u043e\u0431\u0435\u043b\u043e\u0432, \u043c\u044b \u043d\u0435 \u043e\u0431\u044f\u0437\u0430\u043d\u044b \u0437\u0430\u043a\u043b\u044e\u0447\u0430\u0442\u044c <span class=\"arg\">alias_name<\/span> \u0432 \u043a\u0430\u0432\u044b\u0447\u043a\u0438.<br \/>\n  \u041e\u0434\u043d\u0430\u043a\u043e \u0431\u044b\u043b\u043e \u0431\u044b \u0432\u043f\u043e\u043b\u043d\u0435 \u043f\u0440\u0438\u0435\u043c\u043b\u0435\u043c\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u0442\u044c \u044d\u0442\u043e\u0442 \u043f\u0440\u0438\u043c\u0435\u0440, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044f \u043a\u0430\u0432\u044b\u0447\u043a\u0438 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c:<\/p>\n<pre class=\"lang:pgsql decode:true \" >SELECT site_name, COUNT(site_id) AS \"Total\"\r\nFROM sites\r\nGROUP BY site_name;<\/pre>\n<p>  \u0414\u0430\u043b\u0435\u0435 \u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043f\u0440\u0438\u043c\u0435\u0440, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u043c \u043d\u0430\u043c \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0437\u0430\u043a\u043b\u044e\u0447\u0438\u0442\u044c <span class=\"arg\">alias_name<\/span> \u0432 \u043a\u0430\u0432\u044b\u0447\u043a\u0438, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442 \u043f\u0440\u043e\u0431\u0435\u043b\u044b.<br \/>\n  \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440:<\/p>\n<pre class=\"lang:pgsql decode:true \" >SELECT site_name, COUNT(site_id) AS \"Total Sites\"\r\nFROM sites\r\nGROUP BY site_name;<\/pre>\n<p>  \u0412 \u044d\u0442\u043e\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u0435 \u043c\u044b \u043d\u0430\u0437\u0432\u0430\u043b\u0438 \u043f\u043e\u043b\u0435 COUNT(site_id) \u043a\u0430\u043a \"Total Sites\". \u041f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u0432 \u044d\u0442\u043e\u043c <span class=\"arg\">alias_name<\/span> \u0435\u0441\u0442\u044c \u043f\u0440\u043e\u0431\u0435\u043b\u044b, \"Total Sites\" \u0434\u043e\u043b\u0436\u0435\u043d \u0431\u044b\u0442\u044c \u0437\u0430\u043a\u043b\u044e\u0447\u0435\u043d \u0432 \u043a\u0430\u0432\u044b\u0447\u043a\u0438.<\/p>\n<h2>\u041f\u0440\u0438\u043c\u0435\u0440 - \u0430\u043b\u0438\u0430\u0441 \u0442\u0430\u0431\u043b\u0438\u0446\u044b<\/h2>\n<p>  \u041a\u043e\u0433\u0434\u0430 \u0432\u044b \u0441\u043e\u0437\u0434\u0430\u0435\u0442\u0435 \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0434\u043b\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b \u0432 MariaDB, \u044d\u0442\u043e \u043b\u0438\u0431\u043e \u043f\u043e\u0442\u043e\u043c\u0443, \u0447\u0442\u043e \u0432\u044b \u043f\u043b\u0430\u043d\u0438\u0440\u0443\u0435\u0442\u0435 \u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0438\u0442\u044c \u043e\u0434\u043d\u043e \u0438 \u0442\u043e \u0436\u0435 \u0438\u043c\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b \u0431\u043e\u043b\u0435\u0435 \u043e\u0434\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u0430 \u0432 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u0435 FROM (\u0442.\u0435. Self join), \u043b\u0438\u0431\u043e \u0432\u044b \u0445\u043e\u0442\u0438\u0442\u0435 \u0441\u043e\u043a\u0440\u0430\u0442\u0438\u0442\u044c \u0438\u043c\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b, \u0447\u0442\u043e\u0431\u044b \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440 SQL \u0443\u0434\u043e\u0431\u043e\u0447\u0438\u0442\u0430\u0435\u043c\u044b\u043c.<br \/>\n  \u0420\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043f\u0440\u0438\u043c\u0435\u0440 \u0430\u043b\u0438\u0430\u0441\u0430 \u0442\u0430\u0431\u043b\u0438\u0446\u044b \u0432 MariaDB<br \/>\n  \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440:<\/p>\n<pre class=\"lang:pgsql decode:true \" >SELECT pages.page_id, s.site_name\r\nFROM sites s\r\nINNER JOIN pages\r\nON s.site_id = pages.site_id\r\nWHERE s.site_name = 'Google.com'\r\nORDER BY pages.page_id;<\/pre>\n<p>  \u0412 \u044d\u0442\u043e\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u0435 \u043c\u044b \u0441\u043e\u0437\u0434\u0430\u043b\u0438 \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0434\u043b\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b <span class=\"arg\">sites<\/span>, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f <span class=\"arg\">s<\/span>. \u0422\u0435\u043f\u0435\u0440\u044c \u0432\u043d\u0443\u0442\u0440\u0438 \u044d\u0442\u043e\u0433\u043e SQL-\u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u0430 \u043c\u044b \u043c\u043e\u0436\u0435\u043c \u0441\u0441\u044b\u043b\u0430\u0442\u044c\u0441\u044f \u043d\u0430 \u0442\u0430\u0431\u043b\u0438\u0446\u0443 <span class=\"arg\">sites<\/span> \u043a\u0430\u043a <span class=\"arg\">s<\/span>.<br \/>\n  \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0441\u043e\u0437\u0434\u0430\u0442\u044c \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c\u044b \u0434\u043b\u044f \u043b\u044e\u0431\u043e\u0439 \u0438\u043b\u0438 \u0432\u0441\u0435\u0445 \u0442\u0430\u0431\u043b\u0438\u0446.<br \/>\n  \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043c\u044b \u043c\u043e\u0433\u043b\u0438 \u0431\u044b \u0438\u0437\u043c\u0435\u043d\u0438\u0442\u044c \u043d\u0430\u0448 \u043f\u0440\u0438\u043c\u0435\u0440 \u0432\u044b\u0448\u0435 \u0438 \u0441\u043e\u0437\u0434\u0430\u0442\u044c \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0434\u043b\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b <span class=\"arg\">pages<\/span>.<\/p>\n<pre class=\"lang:pgsql decode:true \" >SELECT p.page_id, s.site_name\r\nFROM sites s\r\nINNER JOIN pages p\r\nON s.site_id = p.site_id\r\nWHERE s.site_name = 'Google.com'\r\nORDER BY p.page_id;<\/pre>\n<p>  \u0422\u0435\u043f\u0435\u0440\u044c \u0443 \u043d\u0430\u0441 \u0435\u0441\u0442\u044c \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0434\u043b\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b <span class=\"arg\">pages<\/span> \u0441 \u0438\u043c\u0435\u043d\u0435\u043c <span class=\"arg\">p<\/span>, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043f\u0441\u0435\u0432\u0434\u043e\u043d\u0438\u043c \u0434\u043b\u044f \u0442\u0430\u0431\u043b\u0438\u0446\u044b <span class=\"arg\">sites<\/span> \u0441 \u0438\u043c\u0435\u043d\u0435\u043c <span class=\"arg\">s<\/span>.<\/p>\n","protected":false},"excerpt":{"rendered":"<p>\u0412 \u044d\u0442\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u043e\u043c \u043f\u043e\u0441\u043e\u0431\u0438\u0438 \u0432\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435, \u043a\u0430\u043a \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c MariaDB ALIASES (\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435 \u0438\u043c\u0435\u043d\u0430 \u0441\u0442\u043e\u043b\u0431\u0446\u043e\u0432 \u0438\u043b\u0438 \u0442\u0430\u0431\u043b\u0438\u0446) \u0441 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441\u043e\u043c \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438.<\/p>\n","protected":false},"author":1,"featured_media":0,"comment_status":"closed","ping_status":"closed","sticky":false,"template":"","format":"standard","meta":[],"categories":[281],"tags":[],"_links":{"self":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts\/6431"}],"collection":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/comments?post=6431"}],"version-history":[{"count":0,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\