HERE site_name = name_in;\r\n\r\n   DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE;\r\n\r\n   OPEN c1;\r\n   FETCH c1 INTO TotalSize;\r\n\r\n   CLOSE c1;\r\n\r\n   RETURN TotalSize;\r\n\r\nEND; \/\/\r\n\r\nDELIMITER ;<\/pre>\n","protected":false},"excerpt":{"rendered":"<p>\u0412 \u044d\u0442\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u043e\u043c \u043f\u043e\u0441\u043e\u0431\u0438\u0438 \u0432\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435, \u043a\u0430\u043a \u043d\u0430\u0441\u0442\u0440\u043e\u0438\u0442\u044c \u043e\u0431\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a \u0434\u043b\u044f \u0443\u0441\u043b\u043e\u0432\u0438\u044f NOT FOUND \u0434\u043b\u044f \u043a\u0443\u0440\u0441\u043e\u0440\u0430 \u0432 MariaDB \u0441 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441\u043e\u043c \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438.<\/p>\n","protected":false},"author":1,"featured_media":0,"comment_status":"closed","ping_status":"closed","sticky":false,"template":"","format":"standard","meta":[],"categories":[282],"tags":[],"_links":{"self":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts\/6639"}],"collection":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/comments?post=6639"}],"version-history":[{"count":0,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/posts\/6639\/revisions"}],"wp:attachment":[{"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/media?parent=6639"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/categories?post=6639"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/oracleplsql.ru\/wp-json\/wp\/v2\/tags?post=6639"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templ